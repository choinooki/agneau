{% liquid
  assign enable_burger_menu = false
  for block in section.blocks
    if block.type == 'burger_menu'
      assign enable_burger_menu = true
      assign burger_block = block
    endif
  endfor
%}


<header-drawer data-breakpoint="tablet">
  {% liquid
    assign mega_menu_blocks = false
  %}
  <details class="menu-drawer-container">
    <summary
      class="header__icon header__icon--menu header__icon--summary link--text focus-inset"
      aria-label="{{ 'sections.header.menu' | t }}"
    >
      <span>
        {% render 'icon-hamburger' %}
        <span class="header__modal-close-button">
          {% render 'icon-close' %}
        </span>
      </span>
    </summary>
    <div id="menu-drawer" class="menu-drawer motion-reduce color-background-1" tabind ex="-1">
      <div class="menu-drawer__inner-container">
        <div class="menu-drawer__navigation-container">
          <nav class="menu-drawer__navigation">
            <ul class="menu-drawer__menu list-menu" role="list">
              {%- for link in section.settings.menu.links -%}
                {% liquid
                  assign mega_menu = false
                %}
                <li>
                  {%- if link.links != blank -%}
                    {% for block in section.blocks %}
                      {% liquid
									      assign mega_menu_trigger = block.settings.mega_menu_trigger | handle | strip

                        if link.handle == mega_menu_trigger
                          assign mega_menu = true
                        endif
                      %}
                    {% endfor %}
                    <details>
                      <summary class="menu-drawer__menu-item list-menu__item link--text focus-inset{% if link.child_active %} menu-drawer__menu-item--active{% endif %}">
                        <a
                          href="{{ link.url }}"
                          class="menu-drawer__menu-link h3 link--text list-menu__item focus-inset"
                          {% if childlink.current %}
                            aria-current="page"
                          {% endif %}
                        >
                          {{ link.title | escape }}
                        </a>
                        {% render 'icon-caret' %}
                      </summary>
                      <div
                        id="link-{{ link.title | escape }}"
                        class="menu-drawer__submenu motion-reduce {% if mega_menu_blocks == true %}menu-drawer__submenu--hidden{% endif %}"
                        tabindex="-1"
                      >
                        <div class="menu-drawer__inner-submenu">
                          <button
                            class="menu-drawer__close-button link link--text focus-inset"
                            aria-expanded="true"
                          >
                            {% render 'icon-arrow' %}
                            {{ link.title | escape }}
                          </button>
                          <ul
                            class="menu-drawer__menu list-menu"
                            role="list"
                            tabindex="-1"
                          >
                            {%- for childlink in link.links -%}
                              <li>
                                {%- if childlink.links == blank -%}
                                  <a
                                    href="{{ childlink.url }}"
                                    class="menu-drawer__menu-item h3 link--text list-menu__item focus-inset{% if childlink.current %} menu-drawer__menu-item--active{% endif %}"
                                    {% if childlink.current %}
                                      aria-current="page"
                                    {% endif %}
                                  >
                                    {{ childlink.title | escape }}
                                  </a>
                                {%- else -%}
                                  <details>
                                    <summary class="menu-drawer__menu-item link--text list-menu__item focus-inset">
                                      {{ childlink.title | escape }}
                                      {% render 'icon-caret' %}
                                    </summary>
                                    <div
                                      id="childlink-{{ link.title }}-{{ childlink.title | escape }}"
                                      class="menu-drawer__submenu motion-reduce"
                                    >
                                      <button
                                        class="menu-drawer__close-button link link--text focus-inset"
                                        aria-expanded="true"
                                      >
                                        {% render 'icon-arrow' %}
                                        {{ childlink.title | escape }}
                                      </button>
                                      <ul
                                        class="menu-drawer__menu list-menu"
                                        role="list"
                                        tabindex="-1"
                                      >
                                        {%- for grandchildlink in childlink.links -%}
                                          <li>
                                            <a
                                              href="{{ grandchildlink.url }}"
                                              class="menu-drawer__menu-item h3 link--text list-menu__item focus-inset{% if grandchildlink.current %} menu-drawer__menu-item--active{% endif %}"
                                              {% if grandchildlink.current %}
                                                aria-current="page"
                                              {% endif %}
                                            >
                                              {{ grandchildlink.title | escape }}
                                            </a>
                                          </li>
                                        {%- endfor -%}
                                      </ul>
                                      {% if mega_menu %}
                                        <div class="mega-menu__items">
                                          {% for block in section.blocks %}
                                            {% assign mega_menu_trigger = block.settings.mega_menu_trigger | handle | strip %}
                                            {% if link.handle == mega_menu_trigger %}
                                              {% assign mega_menu_promo_1_trigger = block.settings.mega_menu_promo_1_trigger
                                                | handle
                                                | strip
                                              %}
                                              {% if childlink.handle == mega_menu_promo_1_trigger %}
                                                <div class="mega-menu__item">
                                                  <div class="mega-menu__item-image-wrapper">
                                                    {% if block.settings.image_1 != blank %}
                                                      {{
                                                        block.settings.image_1
                                                        | image_url:
                                                          width: block.settings.image_1.width
                                                        | image_tag:
                                                          loading: 'lazy',
                                                          sizes: '100vw',
                                                          widths: '360, 550, 710',
                                                          width: block.settings.image_1.width,
                                                          class: 'mega-menu__item-image'
                                                      }}
                                                    {%  else %}
                                                      {{ "hero-apparel-1" | placeholder_svg_tag: 'placeholder-svg' }}
                                                    {%- endif -%}
                                                  </div>
                                                  
                                                  {% if block.settings.heading_1 != blank %}
                                                    <div class="mega-menu__item-heading subtitle">
                                                      {{ block.settings.heading_1 }}
                                                    </div>
                                                  {%- endif -%}
                                                  {% if block.settings.button_link_1 != blank %}
                                                    <a
                                                      href="{{ block.settings.button_link_1 }}"
                                                      class="mega-menu__item-button link link--overlay"
                                                    >
                                                    </a>
                                                  {% endif %}
                                                </div>
                                              {% endif %}

                                              {% assign mega_menu_promo_2_trigger = block.settings.mega_menu_promo_2_trigger
                                                | handle
                                                | strip
                                              %}
                                              
                                              {% if childlink.handle == mega_menu_promo_2_trigger %}
                                                <div class="mega-menu__item">
                                                  <div class="mega-menu__item-image-wrapper">
                                                    {% if block.settings.image_2 != blank %}
                                                      {{
                                                        block.settings.image_2
                                                        | image_url:
                                                          width: block.settings.image_2.width
                                                        | image_tag:
                                                          loading: 'lazy',
                                                          sizes: '100vw',
                                                          widths: '360, 550, 710',
