                                assign burger_menu_promo_1_trigger = burger_block.settings.burger_menu_promo_1_trigger | handle | strip
                                assign burger_menu_promo_2_trigger = burger_block.settings.burger_menu_promo_2_trigger | handle | strip
                                assign burger_menu_promo_3_trigger = burger_block.settings.burger_menu_promo_3_trigger | handle | strip
                                assign burger_menu_promo_3_trigger = burger_block.settings.burger_menu_promo_3_trigger | handle | strip
                                assign burger_menu_promo_5_trigger = burger_block.settings.burger_menu_promo_5_trigger | handle | strip
                              %}
                              {% if link.handle == burger_menu_promo_1_trigger or link.handle == burger_menu_promo_2_trigger or link.handle == burger_menu_promo_3_trigger or link.handle == burger_menu_promo_4_trigger or link.handle == burger_menu_promo_5_trigger  %} 
                                <div class="mega-menu__items">
                                  
                                  {% if link.handle == burger_menu_promo_1_trigger   %}
                                    <div class="mega-menu__item">
                                      <div class="mega-menu__image-item">
                                        {% if burger_block.settings.image_1 != blank %}
                                          {{
                                            burger_block.settings.image_1
                                            | image_url:
                                              width: burger_block.settings.image_1.width
                                            | image_tag:
                                              loading: 'lazy',
                                              sizes: '100vw',
                                              widths: '360, 550, 710',
                                              width: burger_block.settings.image_1.width,
                                              class: 'mega-menu__item-image'
                                          }}
                                        {%  else %}
                                          {{ "hero-apparel-1" | placeholder_svg_tag: 'placeholder-svg' }}
                                        {%- endif -%}
                                      </div>
                                      
                                      {% if burger_block.settings.heading_1 != blank %}
                                        <div class="mega-menu__item-heading subtitle">
                                          {{ burger_block.settings.heading_1 }}
                                        </div>
                                      {%- endif -%}
                                      {% if burger_block.settings.button_link_1 != blank %}
                                        <a
                                          href="{{ burger_block.settings.button_link_1 }}"
                                          class="mega-menu__item-button link link--overlay"
                                        >
                                        </a>
                                      {% endif %}
                                    </div>
                                  {% endif %}

                                  {% if link.handle == burger_menu_promo_2_trigger %}
                                    <div class="mega-menu__item">
                                      <div class="mega-menu__image-item ">
                                        {% if burger_block.settings.image_2 != blank %}
                                          {{
                                            burger_block.settings.image_2
                                            | image_url:
                                              width: burger_block.settings.image_2.width
                                            | image_tag:
                                              loading: 'lazy',
                                              sizes: '100vw',
                                              widths: '360, 550, 710',
                                              width: burger_block.settings.image_2.width,
                                              class: 'mega-menu__item-image'
                                          }}
                                        {%  else %}
                                          {{ "hero-apparel-2" | placeholder_svg_tag: 'placeholder-svg' }}
                                        {%- endif -%}
                                      </div>
                                      {% if burger_block.settings.heading_2 != blank %}
                                        <div class="mega-menu__item-heading subtitle">
                                          {{ burger_block.settings.heading_2 }}
                                        </div>
                                      {%- endif -%}
                                      {% if burger_block.settings.button_link_2 != blank %}
                                        <a
                                          href="{{ burger_block.settings.button_link_2 }}"
                                          class="mega-menu__item-button link link--overlay"
                                        >
                                        </a>
                                      {% endif %}
                                    </div>
                                  {% endif %}

                                  {% if link.handle == burger_menu_promo_3_trigger %}
                                    <div class="mega-menu__item">
                                      <div class="mega-menu__image-item ">
                                        {% if burger_block.settings.image_3 != blank %}
                                          {{
                                            burger_block.settings.image_3
                                            | image_url:
                                              width: burger_block.settings.image_3.width
                                            | image_tag:
                                              loading: 'lazy',
                                              sizes: '100vw',
                                              widths: '360, 550, 710',
                                              width: burger_block.settings.image_3.width,
                                              class: 'mega-menu__item-image'
                                          }}
                                        {%  else %}
                                          {{ "hero-apparel-3" | placeholder_svg_tag: 'placeholder-svg' }}
                                        {%- endif -%}
                                      </div>
                                      {% if burger_block.settings.heading_3 != blank %}
                                        <div class="mega-menu__item-heading subtitle">
                                          {{ burger_block.settings.heading_3 }}
                                        </div>
                                      {%- endif -%}
                                      {% if burger_block.settings.button_link_3 != blank %}
                                        <a
                                          href="{{ burger_block.settings.button_link_3 }}"
                                          class="mega-menu__item-button link link--overlay"
                                        >
                                        </a>
                                      {% endif %}
                                    </div>
                                  {% endif %}
                                  
                                  {% if link.handle == burger_menu_promo_4_trigger %}
                                    <div class="mega-menu__item">
                                      <div class="mega-menu__image-item ">
                                        {% if burger_block.settings.image_4 != blank %}
                                          {{
                                            burger_block.settings.image_4
                                            | image_url:
                                              width: burger_block.settings.image_4.width
                                            | image_tag:
                                              loading: 'lazy',
                                              sizes: '100vw',
                                              widths: '360, 550, 710',
                                              width: burger_block.settings.image_4.width,
                                              class: 'mega-menu__item-image'
                                          }}
                                        {%  else %}
                                          {{ "hero-apparel-1" | placeholder_svg_tag: 'placeholder-svg' }}
                                        {%- endif -%}
                                      </div>
                                      {% if burger_block.settings.heading_4 != blank %}
                                        <div class="mega-menu__item-heading subtitle">
                                          {{ burger_block.settings.heading_4 }}
                                        </div>
                                      {%- endif -%}
                                      {% if burger_block.settings.button_link_4 != blank %}
                                        <a
                                          href="{{ burger_block.settings.button_link_4 }}"
                                          class="mega-menu__item-button link link--overlay"
                                        >
                                        </a>
                                      {% endif %}
                                    </div>
                                  {% endif %}

                                  {% if link.handle == burger_menu_promo_5_trigger %}
                                    <div class="mega-menu__item">
                                      <div class="mega-menu__image-item ">
                                        {% if burger_block.settings.image_5 != blank %}
                                          {{
                                            burger_block.settings.image_5
                                            | image_url:
                                              width: burger_block.settings.image_5.width
                                            | image_tag:
                                              loading: 'lazy',
                                              sizes: '100vw',
                                              widths: '360, 550, 710',
                                              width: burger_block.settings.image_5.width,
                                              class: 'mega-menu__item-image'
                                          }}
                                        {%  else %}
                                          {{ "hero-apparel-2" | placeholder_svg_tag: 'placeholder-svg' }}
                                        {%- endif -%}
                                      </div>
                                      {% if burger_block.settings.heading_5 != blank %}
                                        <div class="mega-menu__item-heading subtitle">
                                          {{ burger_block.settings.heading_5 }}
                                        </div>
                                      {%- endif -%}
                                      {% if burger_block.settings.button_link_5 != blank %}
                                        <a
                                          href="{{ burger_block.settings.button_link_3 }}"
                                          class="mega-menu__item-button link link--overlay"
                                        >
                                        </a>
                                      {% endif %}
                                    </div>
                                  {% endif %}
                                </div>
                              {% endif %}
                            {% endif %}
                          </div>
                        </div>
                      </details>
                    {%- else -%}
                      <a
                        href="{{ link.url }}"
                        class="menu-drawer__menu-item list-menu__item h3 link--text focus-inset{% if link.current %} menu-drawer__menu-item--active{% endif %}"
                        {% if link.current %}
                          aria-current="page"
                        {% endif %}
                      >
                        {{ link.title | escape }}
                      </a>
                    {%- endif -%}
                  </li>
                {%- endfor -%}
              {%- endif -%}
            </ul>
            {% if burger_block.settings.small_menu != blank %}
              <ul
                class="burger-menu__list burger-menu__list-second list-unstyled"
                role="list"
              >
                {%- for link in burger_block.settings.small_menu.links -%}
                  <li>
                    {%- if link != blank -%}
                      <a
                        href="{{ link.url }}"
                        class=" burger-menu__item burger-menu__link link-hover-line body-xl"
                        {% if link.current %}
                          aria-current="page"
                        {% endif %}
                      >
                        <span>
                          {{ link.title | escape }}
                        </span>
                      </a>
                    {% endif %}
                  </li>
                {% endfor %}
              </ul>
            {% endif %}
          </nav>

          <div class="menu-drawer__utility-links {% if customer %} menu-drawer__utility-links-customer {% endif %}">
            {%- if shop.customer_accounts_enabled -%}
              {%- if customer -%}
                <div class="menu-drawer__utility-account">
                  <div class="menu-drawer__utility-account-header">
                    <h3 class="menu-drawer__utility-name">
                      {{- customer.name -}}
                    </h3>
  
                  </div>
                  <ul class="menu-drawer__utility-account-body">

                    <li class="menu-drawer__utility-account-link">
                      <a href="{{ routes.account_url }}"  class="link button button--simple">
                        <span>{{ 'customer.account_fallback' | t }}</span>
                      </a>
                    </li>
                    <li class="menu-drawer__utility-account-address">
                      <a href="{{ routes.account_addresses_url }}"  class="link button button--simple">
                        <span>{{- 'customer.addresses.book' | t -}}</span>
                      </a>
                    </li>
                  </ul>
                    <a
                      class="header__account__logout button button--tertiary focus-inset"
                      href="{{ routes.account_logout_url }}"
                    >
                      <span> {{ 'customer.log_out' | t }} </span>
                  </a>
                </div>
              {%- else -%}
                <a
                  href="{%- if customer -%}{{ routes.account_url }}{%- else -%}{{ routes.account_login_url }}{%- endif -%}"
                  class="button button--tertiary menu-drawer__account link focus-inset"
                >
                  <span> 
                    {%- liquid
                      if customer
                        echo 'customer.account_fallback' | t
                      else
                        echo 'customer.log_in' | t
                      endif
                    -%}
                  </span>
                </a>
                {% if settings.enable_create_account %}
                  <div class="menu-drawer__account-register">
                    <a href="{{ routes.account_register_url }}" class="link button button--simple">
                      <span>{{- 'customer.login_page.create_account' | t -}}</span>
                    </a>
                  </div>
                {% endif %}
              {%- endif -%}
            {%- endif -%}
          </div>

          <div class="menu-drawer__bottom">
            {%- if section.settings.enable_country_selector
              or section.settings.enable_language_selector
            -%}
              <div class="menu-drawer__localization-wrapper">
								{% render 'header-region', section: section %}
              </div>
            {%- endif -%}
            {% render 'color-scheme' %}
            {% comment %}{%- if section.settings.enable_search -%}
              <div class="header__icons">
                <div class="header__search">
                  <a
                    href="{{ routes.search_url }}"
                    class="header__icon header__icon--search focus-inset"
                    title="{{ 'general.search.search' | t }}"
                    aria-label="{{ 'general.search.search' | t }}"
                  >
                    <span class="link-hover-line">
                      {{- 'sections.header.search' | t -}}
                    </span>
                  </a>
              </div>
              </div>
            {% endif %}{% endcomment %}
          </div>
        </div>
      </div>
    </div>
  </details>
</header-drawer>
